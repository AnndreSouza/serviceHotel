          <param name="Checkout" style="query" soa:expression="$msg.request/Checkout" type="xsd:date"/>
          <param name="QtdAdultos" style="query" soa:expression="$msg.request/QtdAdultos" type="xsd:int"/>
          <param name="QtdCriancas" style="query" soa:expression="$msg.request/QtdCriancas" type="xsd:int"/>
        </request>
        <response status="200">
          <ns0:representation mediaType="application/json" element="response" xmlns="" xmlns:ns0="http://wadl.dev.java.net/2009/02"/>
        </response>
      </method>
    </resource>
  </resources>
</application>